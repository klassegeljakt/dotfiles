{
  "suggest.maxPreviewWidth": 80,
  "suggest.enablePreview": true,
  "suggest.detailMaxLength": 100,
  "suggest.detailField": "menu",
  "suggest.autoTrigger": "always",
  "suggest.numberSelect": true,
  "suggest.disableKind": false,
  "suggest.disableMenu": false,
  "suggest.snippetIndicator": "ðŸ“’",
  "suggest.maxCompleteItemCount": 50,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.fixInsertedWord": true,
  "suggest.localityBonus": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 500,
  "suggest.minTriggerInputLength": 99,
  "suggest.triggerCompletionWait": 60,
  "suggest.echodocSupport": true,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.noselect": false,
  "suggest.keepCompleteopt": false,
  "suggest.completionItemKindLabels": {},
  "diagnostic.enable": false,
  "diagnostic.displayByAle": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.preferences.snippetStatusText": "SNIP",
  "coc.preferences.formatOnType": false,
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.enableFloatHighlight": true,
  "coc.source.file.enable": true,
  "coc.source.buffer.enable": false,
  "pyls.enable": true,
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {}
    },
    "metals": {
      "command": "metals-vim",
      "rootPatterns": [
        "build.sbt"
      ],
      "filetypes": [
        "scala",
        "sbt"
      ]
    },
//    "rust_analyzer": {
//      "command": "ra_lsp_server",
//      "rootPatterns": [
//        "Cargo.toml"
//      ],
//      "filetypes": [
//        "rust"
//      ]
//    }
  }
}
